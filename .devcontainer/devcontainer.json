{
    "name": "JupyterLab withAgent Client Kernel",
    "build": {
        "dockerfile": "../Dockerfile"
    },
    // "remoteUser": "root",
    "forwardPorts": [
        8888
    ],
    "portsAttributes": {
        "8888": {
            "label": "Jupyter",
            "requireLocalPort": true
        }
    },
    "containerEnv": {
        "JUPYTER_PORT": "8888",
        "GRANT_SUDO": "yes"
        // "RESTARTABLE": "yes"
    }
}
